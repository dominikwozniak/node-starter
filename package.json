{
  "name": "node-setup",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/index.js",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write .",
    "format-lint": "eslint --fix --ext .js,.jsx .",
    "format-all": "npm run format && npm run format-lint",
    "check-format": "prettier --check .",
    "check-lint": "eslint --ext js --ext jsx",
    "check-all": "npm run check-format && npm run check-lint",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/graphql": "^14.5.0",
    "@types/ioredis": "^4.28.7",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.13",
    "@types/redis": "^4.0.11",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.8.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/schema": "^8.3.1",
    "@prisma/client": "^3.8.1",
    "apollo-server": "^3.6.2",
    "apollo-server-core": "^3.6.2",
    "argon2": "^0.28.3",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "graphql": "^16.3.0",
    "graphql-scalars": "^1.14.1",
    "ioredis": "^4.28.4",
    "lodash": "^4.17.21",
    "redis": "^4.0.3",
    "yup": "^0.32.11"
  }
}
